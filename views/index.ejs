<!DOCTYPE html>
<html lang="pt-br">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Playlist</title>
  </head>
  <body>
    <h1>HElloooo EJS</h1>

    <a href="/admin"> Administrador</a>
    <% playlist.forEach((music)=>{ %>
    <section>
      <h2><%= music.name%></h2>
      <p><%= music.author%></p>
      <img src="<%= music.linkImage%>" alt="Ïmagem da musica" />
      <audio controls src="<%= music.linkMusic%>"></audio>
    </section>
    <%})%>
  </body>
</html>
